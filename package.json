    "@tailwindcss/vite": "^4.1.13",
    "axios": "^1.12.2",
    "formik": "^2.4.6",
    "lucide-react": "^0.544.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-quill-new": "^3.6.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.1",
    "react-snap": "^1.23.0",
    "react-toastify": "^11.0.5",
    "react-type-animation": "^3.2.0",
    "tailwindcss": "^4.1.13",
    "yup": "^1.7.1"
